<launch>
        <!-- lidar2imu || chassis2lidar || imu2chassis || multi -->
    <node pkg="lidar_imu_calib" type="calib_exR_lidar2imu_node" name="calib_exR_lidar2imu_node" output="screen">
        <param name="/calib_type" value="multi"/>   
        <param name="/chassis_topic" value="/chassis_msgs"/>   
        <param name="/lidar_topic" value="/velodyne_points"/>
        <param name="/imu_topic" value="/zed2/zed_node/imu/data"/>
        <param name="/bag_file" value="/home/xxiao/dataset/calib/parkinglot.bag"/>
        <!-- <param name="/imu_topic" value="/mti/sensor/imu"/>
        <param name="/bag_file" value="/home/xxiao/dataset/calib/small.bag"/> -->
        <!--simulate/real-->
        <param name="/data_type" value="simulate"/>
        <!--double/multi-->
        <param name="/simulate_type" value="double"/>
        <param name="/file_name1" value="/home/xxiao/dataset/calib/simulate/ex_0.0005/1.txt"/>
        <param name="/file_name2" value="/home/xxiao/dataset/calib/simulate/ex_0.0005/2.txt"/>
        <param name="/file_name3" value="/home/xxiao/dataset/calib/simulate/ex_0.0005/3.txt"/>
    </node>
</launch>